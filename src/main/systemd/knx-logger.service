[Unit]
Description=Log KNX Datagrams

[Service]
ExecStart=/usr/bin/java -jar /opt/knx-logger/MyKnxReader-1.0-SNAPSHOT-jar-with-dependencies.jar -g 192.168.178.1 -j jdbc:mysql://localhost:3306/ZZZZZZ -u YYYYYY -p 'XXXXX' -f /opt/knx-logger/ga.csv -cs ISO-8859-1
Restart=always
RestartSec=5
StartLimitInterval=200
StartLimitBurst=5

[Install]
WantedBy=multi-user.target

